{
  "url":"https://in.linkedin.com/title/{{skill}}-{{role}}/{{location}}",
  "repeatItemGroup": "li.item-container",
  "dataFormat": {
    "name": {
      "selector": ".name",
      "type": "text"
    },
    "link": {
      "selector": ".name > a",
      "type": "attr:href"
    },
    "location": {
      "selector": "div > div.body > dl > dd:nth-child(2)",
      "type": "text"
    },
    "imgUrl":{
      "selector":"img.image",
      "type":"attr:data-delayed-url"
    },
    "source":"linkedin",
    "baseUrl":""
  }
}
